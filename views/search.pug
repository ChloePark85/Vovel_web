extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 #{searchingBy} 로 검색합니다.
    .search__videos
        if videos.length === 0
            h5 해당 키워드에 맞는 영상을 찾을 수 없어요 ㅠ 
        each item in videos 
            +videoBlock({
                title: item.title,
                views: item.views,
                videoFile: item.videoFile,
                id: item.id
            })